    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "semantic-release": "^17.0.7",
    "stylelint": "^13.3.3",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.17.2"
  },
  "keywords": [
    "gatsby",
    "starter",
    "bootstrap"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn format && gatsby build --output-dir",
    "develop": "gatsby develop",
    "format": "yarn format:prettier && yarn format:eslint && yarn format:stylelint",
    "format:eslint": "eslint src/**/*.{js,jsx} --fix",
    "format:prettier": "prettier --write **/*.{js,jsx,json,md,css,scss}",
    "format:stylelint": "stylelint --fix src/**/*.scss",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DecliningLotus/gatsby-lotus-starter"
  },
  "bugs": {
    "url": "https://github.com/DecliningLotus/gatsby-lotus-starter/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
