    "gatsby-transformer-sharp": "^2.5.0",
    "node-sass": "^4.14.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
